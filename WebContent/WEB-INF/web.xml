<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <!--填写项目名，比方说鱼骨图，直方图等--> 
  <display-name>标准操作程序</display-name>
  <!--填写属性标签，为集成与融合做准备-->
   <context-param>
      <param-name>webAppAttributeLabel</param-name>
      <param-value>制表；工具</param-value>
   </context-param>
   <!--功能描述，描述软件的主要功能-->
   <context-param>
      <param-name>webAppDescription</param-name>
      <param-value>将经过不断实践总结出来的，在当前条件下可以实现的最优化的操作程序设计文档化</param-value>
   </context-param>
   <!--webApp图标，从静态资源中选择图片，将图片单独发给我-->
   <context-param>
      <param-name>webAppIcon</param-name>
      <param-value>创新方法管理平台.png</param-value>
   </context-param>
   <!--webApp版本号，当前软件版本迭代次数-->
   <context-param>
      <param-name>webAppVersion</param-name>
      <param-value>v2.0</param-value>
   </context-param>
   <!--webApp分类，从知识工程、TRIZ、质量分析、企业管理、产品设计、创新思维、其他选择一项-->
   <context-param>
      <param-name>webAppCategory</param-name>
      <param-value>知识工程</param-value>
   </context-param>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>